{
  "name": "vite-vue-electron-template",
  "version": "0.0.0",
  "license": "MIT",
  "main": "dist/main/index.js",
  "scripts": {
    "electron:dev": "cross-env NODE_ENV=development electron dist/dev/main/index.js",
    "electron:watch": "rimraf dist/dev && tsc -p tsconfig.electron.json --outDir dist/dev/main -w && rimraf dist/dev",
    "electron:build": "rimraf dist && pnpm vite:build && tsc -p tsconfig.electron.json && electron-builder",
    "vite:dev": "vite",
    "vite:build": "vue-tsc --noEmit && vite build",
    "vite:preview": "vite preview",
    "lint": "eslint --fix --ext .ts,.js,.vue .",
    "format": "prettier -w .",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "vue": "~3.2.25"
  },
  "devDependencies": {
    "@tsconfig/node16": "~1.0.2",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "~5.8.0",
    "@typescript-eslint/parser": "~5.8.0",
    "@vitejs/plugin-vue": "~2.0.0",
    "cross-env": "~7.0.3",
    "electron": "~16.0.5",
    "electron-builder": "~22.14.5",
    "eslint": "~8.5.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-vue": "~8.2.0",
    "prettier": "~2.5.1",
    "rimraf": "~3.0.2",
    "typescript": "~4.4.4",
    "vite": "~2.7.2",
    "vue-tsc": "~0.29.8"
  },
  "engines": {
    "node": "^16.0.0"
  }
}
